<!DOCTYPE html>
<html lang="pl">
<head>
    <meta charset="UTF-8">
    <title>Spadające kwadraciki - {% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        <script src="{{ asset('bundles/fosjsrouting/js/router.min.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
<div class="body-container d-flex">
    {% if form is defined %}{% form_theme form 'form/form_overrides.html.twig' %}{% endif %}
    {% block aside %}{% endblock %}

    <div class="main-container padding-30 w-100">
        <main role="main">
            {% block main %}{% endblock %}
        </main>

        <hr>

        <footer>
            {% block footer %}
                {% include 'component/footer.html.twig' %}
            {% endblock %}
        </footer>
    </div>
</div>
</body>
</html>